{
  "expo": {
    "scheme": "frontend1",
    "userInterfaceStyle": "automatic",
    "orientation": "default",
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font",
      [
      "expo-camera",
        {
         "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
         "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
         "recordAudioAndroid":true,
        }]
    ], 
    "name": "frontend1",
    "slug": "frontend1",
    "ios": {
      "bundleIdentifier": "com.mammo.frontend"
    },
    "android": {
      "package": "com.mammo.frontend"
    }
  }
}
